{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/hotel-helpers.html", "dest": "/public/hotel-helpers.html" },
    { "src": "/car-rental-helpers.html", "dest": "/public/car-rental-helpers.html" },
    { "src": "/ticket-helpers.html", "dest": "/public/ticket-helpers.html" },
    { "src": "/passholder-helper.html", "dest": "/public/passholder-helper.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
